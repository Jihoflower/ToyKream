<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toykream.mapper.product.ProductMapper">

    <insert id="createProduct" parameterType="product" useGeneratedKeys="true" keyProperty="product_id">
        INSERT INTO toyKream.products (product_name,product_img,product_category,product_brand,product_color)
            VALUES (#{productName},#{productImg},#{prodcutCategory},#{prodcutBrand},#{productColor})
    </insert>




</mapper>

