<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toykream.mapper.user.UserMapper">


    <insert id="createMember" parameterType="user" useGeneratedKeys="true" keyProperty="user_id">
        INSERT INTO toyKream.user (username,age) VALUES (#{username},#{age})
    </insert>

<!--    MemberDTO 객체로 전달-->
    <insert id="registMemberDTO" parameterType="member" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO testSchema.member (username,age) VALUES (#{username},#{age})
    </insert>
</mapper>

