<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springTestV2.mapper.MemberMapper">


<!--    model 객체로 전달
    model 객체로 전달 경우 데이터의 이름과 컬럼명이 맞으면 DB 저장이 가능함 -->
    <insert id="registMember" parameterType="member" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO testSchema.member (username,age) VALUES (#{username},#{age})
    </insert>

<!--    MemberDTO 객체로 전달-->
    <insert id="registMemberDTO" parameterType="member" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO testSchema.member (username,age) VALUES (#{username},#{age})
    </insert>
</mapper>

